{
    "manifest_version": 3,
    "name": "Akizuki BOM",
    "description": "copy Akizuki Bom in cart, purchase history",
    "version": "0.91",
    "icons": {
      "128": "128.png"
    },
    "permissions": [
      "notifications",
      "clipboardWrite",
      "tabs",
      "contextMenus"
    ],
    "content_scripts": [{
      "matches": [
        "https://akizukidenshi.com/*"
      ],
      "js": [
        "lib.js",
        "jquery-3.6.4.min.js",
        "contents.js"
      ] 
    }],
    "background": {
      "service_worker": "background.js"
    }
}